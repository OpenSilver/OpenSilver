                                    <Rectangle Margin="1,0,1,1"
                                               x:Name="HoverShineRectangle"
                                               IsHitTestVisible="false"
                                               Opacity="0"
                                               RadiusX="1"
                                               RadiusY="1"
                                               Stroke="{x:Null}"
                                               StrokeThickness="0"
