                                               RadiusY="1"
                                               Stroke="{x:Null}"
                                               StrokeThickness="0"
                                               Fill="{StaticResource ShineBrush}" />
                                    <Rectangle Margin="1,1,1,0"
                                               x:Name="HoverRectangle"
                                               IsHitTestVisible="false"
                                               Opacity="0"
                                               RadiusX="1"
                                               RadiusY="1"
                                               Stroke="{x:Null}"
                                               StrokeThickness="0"
                                               Fill="{StaticResource HoverBrush}" />
                                    <Rectangle Margin="1,1,1,0"
                                               x:Name="HoverShineRectangle"
