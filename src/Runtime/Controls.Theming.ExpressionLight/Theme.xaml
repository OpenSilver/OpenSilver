                                                                  Offset="1" />
                                                    <GradientStop Color="{StaticResource ExpressionTopGradientBrush1}"
                                                                  Offset="0" />
                                                    <GradientStop Color="{StaticResource ExpressionTopGradientBrush1}"
                                                                  Offset="0.356" />
                                                    <GradientStop Color="{StaticResource ExpressionBottomGradientBrush3}"
