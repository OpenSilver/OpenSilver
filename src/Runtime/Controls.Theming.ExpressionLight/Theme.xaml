                                   Grid.Row="2"
                                   Fill="{TemplateBinding SeparatorBrush}" />
                        <ContentPresenter HorizontalAlignment="Center"
                                          VerticalAlignment="Center"
                                          Grid.Column="1"
                                          Grid.RowSpan="2"
                                          Content="{TemplateBinding Content}" />
                        <Path Height="10"
