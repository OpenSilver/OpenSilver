                                                <vsm:VisualTransition GeneratedDuration="0" />
                                                <vsm:VisualTransition GeneratedDuration="00:00:00.1"
                                                                      To="MouseOver" />
                                                <vsm:VisualTransition GeneratedDuration="00:00:00.1"
                                                                      To="Pressed" />
                                                <vsm:VisualTransition From="Normal"
                                                                      GeneratedDuration="00:00:00.3000000"
                                                                      To="MouseOver" />
                                                <vsm:VisualTransition From="MouseOver"
