                                        <LinearGradientBrush EndPoint="0.5,1"
                                                             StartPoint="0.5,0">
                                            <GradientStop Color="#FFFFFFFF"
                                                          Offset="0" />
                                            <GradientStop Color="#FFFEF3B5"
                                                          Offset="0.318" />
                                            <GradientStop Color="#FFFFEB70"
                                                          Offset="0.488" />
                                            <GradientStop Color="#FFFFD02E"
                                                          Offset="0.502" />
                                            <GradientStop Color="#FFFFD932"
                                                          Offset="0.834" />
                                            <GradientStop Color="#FFFFF48B"
                                                          Offset="1" />
                                        </LinearGradientBrush>
                                    </Border.Background>
