                                                            <GradientStop Color="#FFC3BCAE"
                                                                          Offset="0" />
                                                            <GradientStop Color="#FFFDCE9D"
                                                                          Offset="0.046" />
                                                            <GradientStop Color="#FFFFA35B"
                                                                          Offset="0.452" />
                                                            <GradientStop Color="#FFFF8A2C"
                                                                          Offset="0.461" />
                                                            <GradientStop Color="#FFFF9F30"
                                                                          Offset="0.724" />
                                                            <GradientStop Color="#FFFFC472"
                                                                          Offset="1" />
                                                        </LinearGradientBrush>
                                                    </Border.Background>
