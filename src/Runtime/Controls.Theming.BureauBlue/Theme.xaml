                                            Margin="1"
                                            Orientation="Horizontal" />
                                <StackPanel x:Name="PageDisplay"
                                            Orientation="Horizontal">
                                    <TextBlock x:Name="CurrentPagePrefixTextBlock"
                                               Margin="4,0,0,0"
                                               VerticalAlignment="Center"
                                               Width="Auto"
                                               Foreground="{TemplateBinding Foreground}" />
