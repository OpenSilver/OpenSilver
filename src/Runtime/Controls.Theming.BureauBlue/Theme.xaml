                                      Data="M 1,0 L6,0 A 2,2 90 0 1 8,2 L8,7 z" />
                                <Path Fill="#ffffff"
                                      Margin="1,3,0,0"
                                      Data="M 0,0 L2,0 L 8,6 L8,8" />
                            </Grid>
                        </Border>

                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
