                                   Margin="-1,-1,1,0"
                                   Grid.ColumnSpan="2"
                                   Fill="{StaticResource BtnOverFill}"
                                   Stroke="{StaticResource selectedStroke}"
                                   Stretch="Fill"
                                   Opacity="0" />
                        <Rectangle x:Name="BackgroundGradientSelect"
                                   Margin="-1,-1,1,0"
                                   Grid.ColumnSpan="2"
                                   Stretch="Fill"
                                   Fill="{StaticResource hoverGradient}"
                                   Stroke="{StaticResource selectedStroke}"
                                   Opacity="0" />
