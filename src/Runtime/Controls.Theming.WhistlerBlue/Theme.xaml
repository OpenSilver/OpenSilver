                                   Height="21"
                                   Width="11"
                                   Grid.Column="1"
                                   IsTabStop="True" />
                            <RepeatButton x:Name="HorizontalTrackLargeChangeIncreaseRepeatButton"
                                          Grid.Column="2"
                                          IsTabStop="False"
                                          Template="{StaticResource RepeatButtonTemplate}" />
