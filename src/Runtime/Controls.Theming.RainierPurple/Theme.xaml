                                x:Name="Root">
                        <controlsPrimitives:CalendarItem Background="{TemplateBinding Background}"
                                                         BorderThickness="{TemplateBinding BorderThickness}"
                                                         x:Name="CalendarItem" />
                    </StackPanel>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

